  <style media="screen">
    body{
     background-image: linear-gradient(120deg, #89f7fe 0%, #66a6ff 100%);
      box-sizing: border-box;
      font-family: 'Ubuntu', sans-serif;
    }
    .head{
      background-color: white;
      box-shadow: 2px 0 22px grey;
      height: auto;
      width: 100%;
      padding: 10px 30px;
      overflow: auto;
      box-sizing: border-box;
    }
    .logo1{
      width: 100px;
      height: 60px;
      background: url('logo.png');
      background-position: 50% 50%;
      background-size: cover;
      float: left;
    }
    .logo2{
      height: 30px;
      float: left;
      padding: 11px 0 0 10px;
      font-size: 33px;
			color: black;
    }
    .main{
      width: 60%;
      margin: 40px auto;
      overflow: auto;
    }
    .heading{
      text-align: center;
      width: 100%;
      font-size: 38px;
      font-weight: 500;
      color: #555555;
    }
    .booking{
      margin: 20px auto;
      width: 55%;
      text-align: center;
    }
    .from, .to{
      width: 50%;
      float: left;
    }
    .group {
      position:relative;
      float: left;
    }
    input {
      text-transform: uppercase;
      background-color: lightyellow;
      font-size:18px;
      padding:10px 10px 10px 5px;
      display:block;
      width:300px;
      border:none;
      border-top-left-radius: 5px;
			border-top-right-radius: 5px;
			color: black;
    }
    input:focus {
     outline:none; }

    label                {
      color: black;
      font-size:18px;
      font-weight:normal;
      position:absolute;
      pointer-events:none;
      left:5px;
      top:10px;
      transition:0.2s ease all;
    }

    input:focus ~ label, input:valid ~ label        {
      top:-20px;
      font-size:16px;
      color:black !important;
    }

    .bar    { position:relative; display:block; width:300px; }
    .bar:before, .bar:after     {
      content:'';
      height:2px;
      width:0;
      bottom:1px;
      position:absolute;
      background:violet;
      transition:0.2s ease all;
    }
    .bar:before {
      left:50%;
    }
    .bar:after {
      right:50%;
    }

    input:focus ~ .bar:before, input:focus ~ .bar:after {
      width:50%;
    }

    .highlight {
      position:absolute;
      height:60%;
      width:100px;
      top:25%;
      left:0;
      pointer-events:none;
      opacity:0.5;
    }

    input:focus ~ .highlight {
      -webkit-animation:inputHighlighter 0.3s ease;
      -moz-animation:inputHighlighter 0.3s ease;
      animation:inputHighlighter 0.3s ease;
    }

    @-webkit-keyframes inputHighlighter {
        from { background:#5264AE; }
      to    { width:0; background:transparent; }
    }
    @-moz-keyframes inputHighlighter {
        from { background:#526A4E; }
      to    { width:0; background:transparent; }
    }
    @keyframes inputHighlighter {
        from { background:#526A4E; }
      to    { width:0; background:transparent; }
    }

    .ambulances{
      width: 100%;
      padding: 50px 20px;
      overflow: auto;
    }
    .inside{
      background-color: white;
      border-radius: 10px;
      margin-bottom: 40px;
      overflow: none;
      width: 100%;
      height: 90px;
      box-shadow: 0 5px 22px grey, 0 -5px 22px lightgrey;
    }
    .left1, .left2, .left3, .left4, .left5, .left6{
      width: 9%;
      float: left;
      background-size: cover;
      background-position: 50% 50%;
      height: 80px;
    }
    .left1{
      background: url('basicambulance.png');
    }
    .left2{
      background: url('advanceambulance.png');
    }
    .left3{
      background: url('mortuary.png');
    }
    .left4{
      background: url('patientambulance.png');
    }
    .left5{
      background: url('airambulance.png');
    }
    .right{
      width: 90%;
      float: left;
      padding: 15px 30px;
    }
    .right1, .right2, .right3{
      text-align: center;
      width: 33.33%;
      float: left;
    }
    .right1{
      font-size: 23px;
      color: #555555;
      font-weight: 300;
			padding: 15px 1px;
    }
    .right2{
      color: #777;
      font-size: 19px;
			padding: 17px;
    }
    .button{
           display: block;
           text-align: center;
           font-size: 18px;
           margin: auto;
           padding: 4px 30px;
           background-color: #31B9F5;
           border: 0;
           border-radius: 5px;
           cursor: pointer;
           transition: all .5s;
         }
         .button span {
           cursor: pointer;
           display: inline-block;
           position: relative;
           transition: 0.5s;
         }

         .button span:after {
           content: '\00bb';
           position: absolute;
           opacity: 0;
           top: 0;
           right: -20px;
           transition: 0.5s;
         }

         .button:hover span {
           padding-right: 25px;
         }

         .button:hover span:after {
           opacity: 1;
           right: 0;
         }
  </style>
  </head>

  <body>

  <div class="head">
    <div class="logo1">
    </div>
    <div class="logo2">
      Meditek
    </div>
  </div>

  <div class="main">
    <div class="heading">
      Available Ambulances
    </div>
    <hr>

    <div class="booking">
      <div class="from">

        <form>

          <div class="group">
            <input type="text" value="<%=pickup%>" required>
            <span class="highlight"></span>
            <span class="bar"></span>
            <label>From</label>
          </div>

        </form>

      </div>

      <div class="to">

        <form>

          <div class="group">
            <input type="text" value="<%=destination%>" required>
            <span class="highlight"></span>
            <span class="bar"></span>
            <label>To</label>
          </div>

        </form>

      </div>
    </div>

    <div class="ambulances">
  
  
      <% if(basic>0){ var t=Math.floor((Math.random() *10)+5);%>
      <div class="inside">
        <div class="left1">
        </div>
        <div class="right">
          <div class="right1">
            Basic Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
            <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Basic Life Support Ambulance">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
               <button onclick="thankyou('t0')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t0" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>

    <% if(advance>0){ var t=Math.floor((Math.random() *10)+5); %>
      <div class="inside">
        <div class="left2">
        </div>
        <div class="right">
          <div class="right1">
            Advance Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
            <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Advanced Life Support Ambulance">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
               <button onclick="thankyou('t1')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t1" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>

      <% if(mort>0){ var t=Math.floor((Math.random() *10)+5); %>
      <div class="inside">
        <div class="left3">
        </div>
        <div class="right">
          <div class="right1">
            Mortuary Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
            <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Mortuary Ambulance">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
              <!--<button type="submit" class="progress-button" data-style="flip-open" data-perspective data-horizontal>Book Now</button>-->
               <button onclick="thankyou('t2')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t2" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>

      <% if(pta>0){ var t=Math.floor((Math.random() *10)+5); %>
      <div class="inside">
        <div class="left4">
        </div>
        <div class="right">
          <div class="right1">
            Patient Transport Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
            <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Patient Transport Vehicle">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
               <button onclick="thankyou('t4')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t4" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>
    
      <% if(air>0){ var t=Math.floor((Math.random() *10)+5); %>
      <div class="inside">
        <div class="left5">
        </div>
        <div class="right">
          <div class="right1">
            Air Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
            <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Air Ambulance">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
               <button onclick="thankyou('t5')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t5" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>


      <% if(neo>0){ var t=Math.floor((Math.random() *10)+10); %>
      <div class="inside">
        <div class="left6">
					<img src="neo.png" width="90px; height: 90px;">
        </div>
        <div class="right">
          <div class="right1">
            Neonatal Ambulance
          </div>
          <div class="right2">
            <%=t%> min
          </div>
          <div class="right3">
          <form action="/confirmbooking" method="POST">
              <input type="text" style="display:none;" name="type" value="Neonatal Ambulance">
              <input type="text" style="display:none;" name="from" value="<%=pickup%>">
              <input type="text" style="display:none;" name="to" value="<%=destination%>">
              <button onclick="thankyou('t3')" class="progress-button" data-style="flip-open" data-perspective data-horizontal >Book Now</button>
              <button id="t3" style="display:none" type="Submit"></button> 
            </form>
          </div>
        </div>
      </div>
      <% }  %>

    </div>

  </div>

		<script src="https://tympanus.net/Development/ProgressButtonStyles/js/classie.js"></script>
		<script src="https://tympanus.net/Development/ProgressButtonStyles/js/progressButton.js"></script>
		<script>
			[].slice.call( document.querySelectorAll( 'button.progress-button' ) ).forEach( function( bttn ) {
				new ProgressButton( bttn, {
					callback : function( instance ) {
						var progress = 0,
							interval = setInterval( function() {
								progress = Math.min( progress + Math.random() * 0.1, 1 );
								instance._setProgress( progress );

								if( progress === 1 ) {
									instance._stop(1);
									clearInterval( interval );
								}
							}, 200 );
					}
				} );
			} );
			
			
			function thankyou(id){
			  console.log("hello");
			  t = document.getElementById(id);
			  console.log(t)
			  setTimeout(function(){ t.click(); }, 3000);
			}
		</script>

</body>
</html>
